{
    "editor.formatOnSave": true,
    "editor.tabSize": 4,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/node_modules": true,
        "**/bin": true,
        "**/obj": true
    },
    "csharp.suppressHiddenDiagnostics": true,
    "omnisharp.enableRoslynAnalyzers": true,
    "omnisharp.useModernNet": true,
    "dotnet.defaultSolution": "examples/web-sample/web-sample.sln"
}